                    <dt>Cadastro</dt>
                        <dd><%= link_to "Editar Cadastro", :register_edit  %></dd><br />
                    <dt><%= link_to "Contato", :contato %></dt>
                </d1>
            </div>
        </div>

        <div class="column" id="column_right">
          <%= yield %>
        </div>
    </div>

  </div>

</body>
</html>
